for (i in 1:ncol(data)){
  #Phenotype=as.numeric(data$Alanine) #define the phenotype of interest
  my_gwas=gwas2(y=Phenotype, gen=gen, fam=family, chr=chr,EIG=eigNAM) #run the GWAS/NAM analysis
  plot(my_gwas)
  #Sys.sleep(30)
  pval = my_gwas$PolyTest$pval
  results_pvalue[[i]]<-pval
}

results_pvalue
capture.output(results_pvalue, file="pvalue.csv", options(max.print = .Machine$integer.max))


