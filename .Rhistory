library(ggplot2)
library(tidyverse)
library(viridis)
library(magrittr)
library(ggprism)
library(patchwork)
library(ggsignif)
library(extrafont)
loadfonts(device="win")
setwd("C:/Users/sgrfmd/OneDrive - University of Missouri/Desktop/Lab Documents/GWAS/Figures & Tables/ggplot density and boxplot")
data<-read.csv("Cd111 Alleleic Input.csv", header=TRUE)
head(data)
XVAR<-data$Cd111
YVAR<-data$ALLELE
med<-aggregate(XVAR ~ YVAR, data, median)
Z<-ggplot(data, aes(x=XVAR, y=YVAR, color=YVAR))+geom_boxplot(aes(color=YVAR))+geom_jitter(aes(color=YVAR))+stat_boxplot(mapping=NULL, data=NULL, geom="errorbar", width=0.3)+geom_signif(comparisons=list(c("G", "A")), map_signif_level = TRUE, textsize = 16, family = "TT Times New Roman", color="black")
print(Z)
ZZ<-Z+geom_text(data=med, aes(label=XVAR), color="black", size=5 +0.08, vjust=0, family="Times New Roman")
ZZZ<-ZZ+theme_minimal(base_size = 16, base_family = "TT Times New Roman")+coord_flip()+labs(x="Cd Content (mg/kg)", y="SNP: Gm09_5034794")+scale_color_discrete("Allele")+theme(axis.text.x=element_text(color = "black"))+theme(axis.text.y = element_text(color="black"))
print(ZZZ)
ZZZ+xlim(0.0,0.20)
